{
  "version": 2,
  "projects": {
    "mtl-app": {
      "root": "apps/mtl-app",
      "sourceRoot": "apps/mtl-app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/mtl-app",
            "outputPath": "dist/apps/mtl-app"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "mtl-app:build",
            "dev": true,
            "port": 3000
          },
          "configurations": {
            "production": {
              "buildTarget": "mtl-app:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "mtl-app:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/mtl-app"],
          "options": {
            "jestConfig": "apps/mtl-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mtl-app/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    },
    "mtl-app-e2e": {
      "root": "apps/mtl-app-e2e",
      "sourceRoot": "apps/mtl-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mtl-app-e2e/cypress.json",
            "tsConfig": "apps/mtl-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "mtl-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mtl-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mtl-app-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": [],
      "implicitDependencies": ["mtl-app"]
    },
    "mtl-server": {
      "root": "apps/mtl-server",
      "sourceRoot": "apps/mtl-server/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/mtl-server",
            "main": "apps/mtl-server/src/main.ts",
            "tsConfig": "apps/mtl-server/tsconfig.app.json",
            "assets": ["apps/mtl-server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/mtl-server/src/environments/environment.ts",
                  "with": "apps/mtl-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "mtl-server:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/mtl-server/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/mtl-server"],
          "options": {
            "jestConfig": "apps/mtl-server/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
