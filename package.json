  "scripts": {
    "nx": "nx",
    "dev:app": "nx serve",
    "dev:server": "start mtl-server",
    "start:server:prod": "node dist/apps/mtl-server/main.js",
    "build": "nx build",
    "build:server": "yarn build mtl-server --prod",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prisma-seed": "yarn prisma db seed",
    "prisma-update": "yarn prisma-generate && env-cmd yarn prisma-migrate",
    "prisma-generate": "prisma generate --schema=apps/mtl-app/prisma/schema.prisma",
    "prisma-migrate": "prisma migrate deploy --schema=apps/mtl-app/prisma/schema.prisma",
    "postinstall": "yarn prisma-generate",
    "docker:build:mtl-app": "docker build -t mtl-app -f apps/mtl-app/Dockerfile .",
    "docker:build:mtl-server": "docker build -t mtl-server -f apps/mtl-server/Dockerfile .",
    "docker:build:mtl-screenshot-api": "docker build -t mtl-screenshot-api -f apps/mtl-screenshot-api/Dockerfile .",
    "docker:push:mtl-app": "docker tag mtl-app registry.digitalocean.com/mtl-container-registry/mtl-app && docker push registry.digitalocean.com/mtl-container-registry/mtl-app",
    "docker:push:mtl-server": "docker tag mtl-server registry.digitalocean.com/mtl-container-registry/mtl-server && docker push registry.digitalocean.com/mtl-container-registry/mtl-server",
    "docker:push:mtl-screenshot-api": "docker tag mtl-screenshot-api registry.digitalocean.com/mtl-container-registry/mtl-screenshot-api && docker push registry.digitalocean.com/mtl-container-registry/mtl-screenshot-api",
    "docker:run:mtl-app": "docker run --env-file ./.env -p 3000:3000 mtl-app",
    "docker:run:mtl-server": "docker run --env-file ./.env -p 3001:3001 mtl-server",
    "docker:run:mtl-screenshot-api": "docker run --env-file ./.env -p 3002:3002 mtl-screenshot-api"
  },
  "prisma": {
    "schema": "apps/mtl-app/prisma/schema.prisma",
    "seed": "ts-node --project apps/mtl-app/tsconfig.json --compiler-options {\"module\":\"CommonJS\"} apps/mtl-app/prisma/seed.ts"
  },
