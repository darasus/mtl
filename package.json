    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prisma-seed": "yarn prisma db seed",
    "prisma-update": "yarn prisma-generate && env-cmd yarn prisma-migrate",
    "prisma-generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate --schema=apps/mtl-app/prisma/schema.prisma",
    "prisma-migrate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy --schema=apps/mtl-app/prisma/schema.prisma",
    "postinstall": "yarn prisma-generate",
    "docker:build": "docker build -t mtl-server -f apps/mtl-server/Dockerfile.mtl-server .",
    "docker:run": "docker run -p 3002:3001 --env-file ./.env mtl-server",
    "deploy:server": "flyctl deploy --config ./apps/mtl-server/fly.toml --dockerfile ./apps/mtl-server/Dockerfile.mtl-server"
  },
  "prisma": {
    "schema": "apps/mtl-app/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
